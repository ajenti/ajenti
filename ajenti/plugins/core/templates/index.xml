<html>
<head id="_head">
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <headstylesheets/>
    <headscripts/>
    <link rel="shortcut icon" href="/dl/core/ui/favicon.png" />
</head>
<body id="main">

<div id="page">
    <div id="topbar">
        <div>
            <img class="ui-el-logo" src="/dl/core/ui/logo.png" />
            Ajenti
            <img class="ui-el-ajax-loader" src="/dl/core/ui/ajax.gif" id="ajax-loader" style="display:none"/>
        </div>
    </div>

    <div id="topbar2">
        <div class="wr">
            <span id="topplaceholder-top"/>
            <span id="topplaceholder-bottom"/>
        </div>
    </div>

    <div id="topbar3">
    </div>
    
    <div class="ui-el-mainwindow-body">
		<div class="ui-el-mainwindow-left" id="leftplaceholder"/>
		<div class="ui-el-mainwindow-right" id="rightplaceholder"> 
		    <script>
		        ajax('/handle///nothing');
		    </script>
		</div>
		<div style="clear: both"/> 
	</div>
		
    <div class="ui-el-mainwindow-bottom">
        <div class="wr">
            <div class="ui-el-bottombar-l" id="version" /> 
            <div class="ui-el-bottombar-r" id="links" /> 
        </div>
    </div>
        
    <script>
        ajax('/handle///nothing');
    </script>        
        
<div id="warningbox-all" style="display:none">
    <div class="ui-el-modal-blackout" />
    <div class="ui-el-modal-wrapper" id="warningbox-wr">
        <div class="ui-el-dialog">
            <div class="ui-el-dialog-content">
                <table>
                    <tr>
                        <td width="40">
                            <img src="/dl/core/ui/stock/warning.png" />
                        </td>
                        <td>
                            <div class="ui-el-error-title">
                                <span class="ui-el-label-3">You are about to:</span>
                            </div>
                            <div class="ui-el-error-content" style="min-width:0px">
                                <span id="warning-text" />
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
            <div class="ui-el-modal-buttons">
                <button text="OK" id="warning-button"/>
                <a class="ui-el-button" href="#" id="{@id}" onclick="ui_hidewarning()">
                    Cancel
                </a>
            </div>
        </div>
    </div>
</div>

<div id="help-hint" class="ui-el-helphint"></div>

</div>

</body>
</html>

